/**app.wxss**/
.container {
  height: 100%;
  display: flex;
  flex-direction: column;
  align-items: center;
  justify-content: space-between;
  padding: 200rpx 0;
  box-sizing: border-box;
} 
page {
  background-color: #f5f5f9;
  color: #333;
}
.btn-exchange {
  font-size: 24rpx;
  text-align: center;
  line-height: 58rpx;
  height: 58rpx;
  width: 158rpx;
  border-radius: 29rpx;
  float: right;
  margin-top: 30rpx;
}
.exchange {
  color: #42bd56;
  border: 1rpx #42bd56 solid;
}
.exchanged {
  background-color: #ebebf0;
  border: 1rpx #ebebf0 solid;
}
.bgAttr {
  background-size: cover;
  background-repeat: no-repeat;
  background-position: center;
}
.mask {
  position: absolute;
  top: 0;
  height: 100%;
  width: 100%;
  background-color: rgba(0, 0, 0, 0.7);
}